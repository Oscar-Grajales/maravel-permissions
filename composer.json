{
    "name": "inani/maravel-permissions",
    "description": "A Laravel Package that handle roles and permissions for the user",
    "keywords": ["role", "permission", "ability" ,"user", "laravel", "php"],
    "license": "MIT",
    "authors": [
        {
            "name": "Inani El Houssain",
            "email": "inanielhoussain@gmail.com"
        }
    ],
    "homepage": "https://github.com/akiyamaSM/maravel-permissions",
    "minimum-stability": "stable",
    "autoload": {
      "psr-4": {
        "Inani\\Maravel\\": "src"
      }
    },
    "require": {
      "illuminate/support": "~9|~10|~11"
    },
    "require-dev": {
      "phpunit/phpunit": "~9|~10|~11"
    },
    "extra": {
      "laravel": {
        "providers": [
          "Inani\\Maravel\\Providers\\MaravelServiceProvider"
        ]
      }
    },
    "config": {
        "sort-packages": true
    }
}
